<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Html\Calendar" name="html_calendar" as="html_calendar"
                   template="Magento_Theme::js/calendar.phtml"/>
            <block name="helpdesk.ticket.list" class="Mirasvit\Helpdesk\Block\Ticket\Create"
                   template="ticket/create.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>
