<?xml version="1.0"?>
<config>
    <modules>
        <Gearup_Missingattribute>
            <version>1.0.0</version>
        </Gearup_Missingattribute>
    </modules>
    <global>
        <models>
            <missingattribute>
                <class>Gearup_Missingattribute_Model</class>
                <resourceModel>missingattribute_mysql4</resourceModel>
            </missingattribute>
        </models>
        <helpers>
            <missingattribute>
              <class>Gearup_Missingattribute_Helper</class>
            </missingattribute>
      </helpers>
    </global>    
    <crontab>
        <jobs>            
            <missingattribute_run>
                <schedule>
                    <cron_expr>0 9 * * *</cron_expr>
                </schedule>
                <run>
                    <model>missingattribute/cron::Run</model>
                </run>
            </missingattribute_run>
            <missingimage_run>
                <schedule>
                    <cron_expr>40 9 * * *</cron_expr>
                </schedule>
                <run>
                    <model>missingattribute/cron::Runimage</model>
                </run>
            </missingimage_run>
        </jobs>
    </crontab>
</config> 