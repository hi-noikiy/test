 <config>
    <menu>
         <competera translate="title" module="competera">
            <title>Competera</title>
            <sort_order>70</sort_order>
            <children>
                <priceload translate="title" module="competera">
                    <title>Priceload</title>
                    <action>adminhtml/competera_priceload</action>
                    <sort_order>0</sort_order>
                </priceload>
                <history translate="title" module="competera">
                    <title>Review History</title>
                    <action>adminhtml/competera_history</action>
                    <sort_order>10</sort_order>
                </history>
            </children>
         </competera>
    </menu>
    <acl>
        <resources>
            <admin>
                <children>
                    <competera translate="title" module="competera">
                        <title>Competera</title>
                        <sort_order>70</sort_order>
                        <children>
                            <priceload translate="title">
                                <title>Priceload</title>
                                <sort_order>10</sort_order>
                            </priceload>
                            <history translate="title">
                                <title>Review History</title>
                                <sort_order>20</sort_order>
                            </history>
                            <pricechange translate="title">
                                <title>Price Changelog</title>
                                <sort_order>30</sort_order>
                            </pricechange>
                        </children>
                    </competera>
                </children>
            </admin>
        </resources>
    </acl>
</config>